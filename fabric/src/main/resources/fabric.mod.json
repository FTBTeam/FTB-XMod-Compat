{
  "schemaVersion": 1,
  "id": "ftbxmodcompat",
  "version": "${version}",
  "name": "FTB XMod Compat",
  "description": "Provides cross-mod compatibility/integration for all main FTB mods",
  "authors": [
    "FTB Team"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "All Rights Reserved",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.ftb.mods.ftbxmodcompat.fabric.FTBXModCompatFabric"
    ],
    "rei_client": [
      "dev.ftb.mods.ftbxmodcompat.ftbquests.rei.FTBQuestsREIIntegration"
    ]
  },
  "depends": {
    "fabric": "${fabricversionrange}",
    "minecraft": ">=${mcversion}",
    "architectury": ">=${archversion}",
    "ftblibrary": ">=${ftblibraryversion}"
  },
  "breaks": {
    "ftbquests": "<2001.3.0",
    "ftbchunks": "<2001.2.0",
    "jei": "<15.2",
    "kubejs": "<2001.6.3-build.18",
    "waystones": "<${required_waystones_version}"
  }
}